<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project hc3d-old">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.buildfolder" value="C:/Users/Joel/Documents/hc3d/build"/>
    <target name="create_run_jar">
        <jar destfile="${dir.buildfolder}/hc3d.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="io.infinus.hc3d.Main"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.buildfile}/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/gst/gst1-java-core-1.1.0.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jna/android-arm.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jna/android-armv7.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jna/jna.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/gst/gst1-java-swing-0.9.0.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/bridj-0.6.2.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/slf4j-api-1.7.2.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/webcam-capture-0.3.10.jar"/>
        </jar>
    </target>
</project>
